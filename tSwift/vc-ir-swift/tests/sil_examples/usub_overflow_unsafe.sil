// SIL test file: Unsigned subtraction with overflow check
// This demonstrates the overflow check that Swift generates for UInt subtraction

sil_stage canonical

import Builtin
import Swift

// Simple unsigned sub function with overflow check
// Unsigned subtraction can overflow when:
// - a - b < 0 (wraps to large positive, e.g., 0 - 1 = UINT64_MAX)
sil hidden @usub : $@convention(thin) (UInt, UInt) -> UInt {
bb0(%0 : $UInt, %1 : $UInt):
  debug_value %0, let, name "a", argno 1
  debug_value %1, let, name "b", argno 2
  %4 = struct_extract %0, #UInt._value
  %5 = struct_extract %1, #UInt._value
  %6 = integer_literal $Builtin.Int1, -1
  %7 = builtin "usub_with_overflow_Int64"(%4, %5, %6) : $(Builtin.Int64, Builtin.Int1)
  %8 = tuple_extract %7, 0
  %9 = tuple_extract %7, 1
  cond_fail %9, "arithmetic overflow"
  %11 = struct $UInt (%8)
  return %11
}
