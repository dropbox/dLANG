// SIL test file: UInt16 Subtraction with overflow check
// Tests that unsigned bitwidth-specific bounds (0 to 65535) are used

sil_stage canonical

import Builtin
import Swift

// UInt16 sub function with overflow check
// Unsigned subtraction can overflow (underflow) when:
// - a - b < 0 (e.g., 0 - 1 wraps to 65535 if unchecked)
sil hidden @sub_uint16 : $@convention(thin) (UInt16, UInt16) -> UInt16 {
bb0(%0 : $UInt16, %1 : $UInt16):
  debug_value %0, let, name "a", argno 1
  debug_value %1, let, name "b", argno 2
  %4 = struct_extract %0, #UInt16._value
  %5 = struct_extract %1, #UInt16._value
  %6 = integer_literal $Builtin.Int1, -1
  %7 = builtin "usub_with_overflow_Int16"(%4, %5, %6) : $(Builtin.Int16, Builtin.Int1)
  %8 = tuple_extract %7, 0
  %9 = tuple_extract %7, 1
  cond_fail %9, "arithmetic overflow"
  %11 = struct $UInt16 (%8)
  return %11
}
