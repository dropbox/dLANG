// SIL test file: UInt32 unsigned division by zero detection
// Tests that div-by-zero auto-VC is generated for udiv_Int32

sil_stage canonical

import Builtin
import Swift

// UInt32 unsigned division - may divide by zero
sil hidden @div_uint32 : $@convention(thin) (UInt32, UInt32) -> UInt32 {
bb0(%0 : $UInt32, %1 : $UInt32):
  debug_value %0, let, name "a", argno 1
  debug_value %1, let, name "b", argno 2
  %4 = struct_extract %0, #UInt32._value
  %5 = struct_extract %1, #UInt32._value
  %6 = builtin "udiv_Int32"(%4, %5) : $Builtin.Int32
  %7 = struct $UInt32 (%6)
  return %7
}
