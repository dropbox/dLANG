// SIL test file: Int32 left shift with unconstrained shift amount (UNSAFE)
// No precondition constrains the shift amount - could be negative or >= 32.
// Auto-VC for shift overflow should FAIL because shift amount is unbounded.

sil_stage canonical

import Builtin
import Swift

// Unsafe Int32 shift: no constraint on shift amount
func unsafeInt32ShiftLeft(_ x: Int32, _ s: Int32) -> Int32

sil hidden @unsafeInt32ShiftLeft : $@convention(thin) (Int32, Int32) -> Int32 {
bb0(%0 : $Int32, %1 : $Int32):
  debug_value %0, let, name "x", argno 1
  debug_value %1, let, name "s", argno 2
  %4 = struct_extract %0, #Int32._value
  %5 = struct_extract %1, #Int32._value
  %6 = builtin "shl_Int32"(%4, %5) : $Builtin.Int32
  %7 = struct $Int32 (%6)
  return %7
}
