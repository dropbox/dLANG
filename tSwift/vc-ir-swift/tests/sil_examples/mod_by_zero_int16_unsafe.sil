// SIL test file: Int16 signed modulo by zero detection
// Tests that mod-by-zero auto-VC is generated for srem_Int16

sil_stage canonical

import Builtin
import Swift

// Int16 signed modulo - may divide by zero
sil hidden @mod_int16 : $@convention(thin) (Int16, Int16) -> Int16 {
bb0(%0 : $Int16, %1 : $Int16):
  debug_value %0, let, name "a", argno 1
  debug_value %1, let, name "b", argno 2
  %4 = struct_extract %0, #Int16._value
  %5 = struct_extract %1, #Int16._value
  %6 = builtin "srem_Int16"(%4, %5) : $Builtin.Int16
  %7 = struct $Int16 (%6)
  return %7
}
